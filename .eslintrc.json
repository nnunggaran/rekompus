{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "no-restricted-globals": 0,
        "no-use-before-define": 0,
        "func-names": 0,
        "no-undef": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "global-require": 0,
        "no-unused-vars":0
    }
}
